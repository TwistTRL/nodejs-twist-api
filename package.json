    "stage": "pm2 start ecosystem.config.js --env stage",
    "dev": "nodemon index.js",
    "docs": "node replace-string.js && apidoc -e node_modules -c config/apidoc-config -i ./ -o docs/apidoc && apidoc -e node_modules -c config/apidoc2-config -i ./ -o docs/apidoc2 -t template/",
