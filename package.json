    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "is-my-json-valid": "^2.20.0",
    "kill-port": "^1.6.0",
    "morgan": "^1.9.1",
    "oracledb": "^4.2.0",
    "pm2": "^4.2.3",
    "relational-query-sql-builder": "github:TwistTRL/relational-query-sql-builder",
